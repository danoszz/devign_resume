{
  "name": "devignit_resume",
  "version": "1.0.0",
  "description": "Digital resume for summer 2018",
  "author": "Daan van der Zwaag - daan@devign.it",
  "license": "MIT",
  "homepage": "https://resume.devign.it",
  "keywords": [
    "gatsby",
    "contentful"
  ],
  "main": "n/a",
  "repository": {
    "type": "git",
    "url": "git@github.com:danoszz/devign_resume.git"
  },
  "scripts": {
    "dev": "gatsby develop",
    "lint": "eslint --ext .js,.jsx --ignore-pattern public .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "gatsby build",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public",
    "fix-semi": "eslint --quiet --ignore-pattern node_modules --ignore-pattern public --parser babel-eslint --no-eslintrc --rule '{\"semi\": [2, \"never\"], \"no-extra-semi\": [2]}' --fix *.js bin/*.js",
    "setup": "node ./bin/setup.js",
    "heroku-postbuild": "gatsby build",
    "cleanup-repository": "rimraf bin contentful && yarn remove contentful-import chalk inquirer rimraf "
  },
  "dependencies": {
    "@contentful/gatsby-transformer-contentful-richtext": "^13.1.0",
    "@contentful/rich-text-react-renderer": "^13.2.0",
    "gatsby-cli": "^2.7.15",
    "gatsby-image": "^2.2.6",
    "gatsby-plugin-react-helmet": "^3.1.2",
    "gatsby-plugin-sass": "^2.1.3",
    "gatsby-plugin-sharp": "^2.2.7",
    "gatsby-plugin-typescript": "^2.1.2",
    "gatsby-source-contentful": "^2.1.13",
    "gatsby-source-filesystem": "^2.1.5",
    "gatsby-transformer-remark": "^2.6.6",
    "gatsby-transformer-sharp": "^2.2.3",
    "gsap": "^2.1.3",
    "jquery": "^3.4.1",
    "lodash": "^4.17.14",
    "node-sass": "^4.12.0",
    "npm-check-updates": "^3.1.20",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.1",
    "sharp": "^0.22.1",
    "styled-components": "^4.3.2",
    "swiper": "^4.5.0",
    "typescript": "^3.5.3"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.2",
    "contentful-import": "^7.5.1",
    "dotenv": "^8.0.0",
    "eslint": "^6.0.1",
    "eslint-plugin-react": "^7.14.2",
    "gatsby": "^2.13.25",
    "gh-pages": "^2.0.1",
    "prettier": "^1.18.2",
    "yargs-parser": "^13.1.1"
  }
}
